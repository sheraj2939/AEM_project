<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Table Component"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
            <column
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                    <tabs
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/tabs">
                        <items jcr:primaryType="nt:unstructured">
                            <popup-video
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Popup Video Content"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                composite="{Boolean}true"
                                fieldLabel="Popup Video Content">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <video-heading
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Title (Regular)"
                                                name="./videotitle"/>
                                            <thumbnail
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                fieldDescription="Please Upload Image As Per Card Variation"
                                                fieldLabel="Thumbnail Image"
                                                name="./popupThumbnailUrl"/>
                                            <video-link
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Video Link"
                                                name="./popupVideoUrl"/>
                                            <video-upload-date
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Video Upload Date"
                                                name="./youtubeUploadDate"/>
                                            <video-duration
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Video Duration"
                                                name="./youtubeVideoTime"/>
                                        </items>
                                    </column>
                                </items>
                            </popup-video>
                            <ads-video
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Ads Video Content"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                composite="{Boolean}true"
                                fieldLabel="Ads Video Content">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <video-heading
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Title (Regular)"
                                                name="./videotitle"/>
                                            <video-link
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Video Link"
                                                name="./adsVideoUrl"/>
                                            <video-upload-date
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Video Upload Date"
                                                name="./youtubeUploadDate"/>
                                            <video-duration
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Video Duration"
                                                name="./youtubeVideoTime"/>
                                        </items>
                                    </column>
                                </items>
                            </ads-video>
                        </items>
                    </tabs>
                </items>
            </column>
        </items>
    </content>
</jcr:root>
