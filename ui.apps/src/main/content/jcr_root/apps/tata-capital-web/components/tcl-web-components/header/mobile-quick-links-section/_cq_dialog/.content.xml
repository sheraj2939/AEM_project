<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Header Mobile Authoring"
    sling:resourceType="cq/gui/components/authoring/dialog"
    helpPath="">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
            <column
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                    <Quick-links-section
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/accordion">
                        <items jcr:primaryType="nt:unstructured">
                            <container
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Quick Links Section"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <navHeading
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Enter Title for Investment Section"
                                        name="./quickLinksTitle"
                                        useFixedInlineToolbar="{Boolean}true"/>
                                    <nav-redirection-link
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                        fieldLabel="Enter redirection link"
                                        name="./navQuickLinksTitleRedirectionLink"
                                        rootPath=""/>
                                    <tab-menu
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/accordion">
                                        <items jcr:primaryType="nt:unstructured">
                                            <container
                                                jcr:primaryType="nt:unstructured"
                                                jcr:title="Tab Menu"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <tab-menu
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                        composite="{Boolean}true"
                                                        fieldLabel="Tab Menu Authoring">
                                                        <field
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/fieldset"
                                                            name="./tabMenuQuciLinksection">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <column
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/container">
                                                                    <items jcr:primaryType="nt:unstructured">
                                                                        <tab-menu-Item-title
                                                                            jcr:primaryType="nt:unstructured"
                                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                            fieldLabel="Menu Item Tilte"
                                                                            name="./tabMenuItemTitle"
                                                                            useFixedInlineToolbar="{Boolean}true"/>
                                                                        <need-p-tag
                                                                            jcr:primaryType="nt:unstructured"
                                                                            sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                                            composite="{Boolean}true"
                                                                            name="./pTagNeed"
                                                                            text="Title in p tag"
                                                                            value="true"/>
                                                                        <tab-item-content
                                                                            jcr:primaryType="nt:unstructured"
                                                                            sling:resourceType="granite/ui/components/coral/foundation/accordion">
                                                                            <items jcr:primaryType="nt:unstructured">
                                                                                <container
                                                                                    jcr:primaryType="nt:unstructured"
                                                                                    jcr:title="Tab Item Content Authoring"
                                                                                    sling:resourceType="granite/ui/components/coral/foundation/container">
                                                                                    <items jcr:primaryType="nt:unstructured">
                                                                                        <subLink
                                                                                            jcr:primaryType="nt:unstructured"
                                                                                            sling:resourceType="granite/ui/components/coral/foundation/accordion">
                                                                                            <items jcr:primaryType="nt:unstructured">
                                                                                                <subLink
                                                                                                    jcr:primaryType="nt:unstructured"
                                                                                                    jcr:title="Calculator List"
                                                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                                                                    composite="{Boolean}true"
                                                                                                    fieldLabel="Quick Link List">
                                                                                                    <field
                                                                                                        jcr:primaryType="nt:unstructured"
                                                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/fieldset"
                                                                                                        name="./quickLinkItem">
                                                                                                        <items jcr:primaryType="nt:unstructured">
                                                                                                            <column
                                                                                                                jcr:primaryType="nt:unstructured"
                                                                                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                                                                                <items jcr:primaryType="nt:unstructured">
                                                                                                                    <topHeading
                                                                                                                        jcr:primaryType="nt:unstructured"
                                                                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                                                                        fieldLabel="Enter qucik Link Title"
                                                                                                                        name="./quickLinkItemTitle"
                                                                                                                        useFixedInlineToolbar="{Boolean}true"/>
                                                                                                                    <iconClass
                                                                                                                        jcr:primaryType="nt:unstructured"
                                                                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                                                                        fieldLabel="Enter class of icon"
                                                                                                                        name="./iconClass"
                                                                                                                        useFixedInlineToolbar="{Boolean}true"/>
                                                                                                                    <link
                                                                                                                        jcr:primaryType="nt:unstructured"
                                                                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                                                                                        fieldLabel="Enter redirection link"
                                                                                                                        name="./redirectionLink"
                                                                                                                        rootPath=""/>
                                                                                                                    <qucik-link-open-new-Tab
                                                                                                                        jcr:primaryType="nt:unstructured"
                                                                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                                                                                        composite="{Boolean}true"
                                                                                                                        name="./quickrLinkOpeninNewTab"
                                                                                                                        text="Open in New Tab"
                                                                                                                        value="true"/>
                                                                                                                </items>
                                                                                                            </column>
                                                                                                        </items>
                                                                                                    </field>
                                                                                                </subLink>
                                                                                            </items>
                                                                                        </subLink>
                                                                                    </items>
                                                                                </container>
                                                                            </items>
                                                                        </tab-item-content>
                                                                    </items>
                                                                </column>
                                                            </items>
                                                        </field>
                                                    </tab-menu>
                                                </items>
                                            </container>
                                        </items>
                                    </tab-menu>
                                </items>
                            </container>
                        </items>
                    </Quick-links-section>
                </items>
            </column>
        </items>
    </content>
</jcr:root>
