<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Wealth Services"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
            <column
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                    <wealthService
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Wealth Service Boxes"
                        sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                        composite="{Boolean}true"
                        eaem-show-on-collapse="EAEM.showProductName"
                        fieldLabel="Boxes Multifeild">
                        <field
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/fieldset"
                            eaem-nested="NODE_STORE"
                            name="./wealthBox">
                            <items jcr:primaryType="nt:unstructured">
                                <column
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/container">
                                    <items jcr:primaryType="nt:unstructured">
                                        <image
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                            fieldLabel="Box Background Image"
                                            name="./bgImage"
                                            rootPath="/content/dam/tata-capital-web"/>
                                        <bgImgAlt
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldLabel="Background Image Alt"
                                            name="./bgImgAlt"/>
                                        <logoImage
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                            fieldLabel="Logo Image"
                                            name="./logoImage"
                                            rootPath="/content/dam/tata-capital-web"/>
                                        <logoAlt
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldLabel="Logo Image Alt"
                                            name="./logoAlt"/>
                                        <title
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldLabel="Title"
                                            name="./boxTitle"/>
                                        <semobilTitle
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldLabel="Semibold Title"
                                            name="./semoBoldTitle"/>
                                        <primaryButtonTitle
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldLabel="Primary Button Title"
                                            name="./primaryButtonTitle"/>
                                        <primaryButtonLink
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                            fieldLabel="Primary Button Link"
                                            name="./primaryButtonLink"/>
                                        <primaryButtonlinkOpenInNewTab
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                            composite="{Boolean}true"
                                            name="./primaryButtonLinkOpenInNewTab"
                                            text="Open Primary Button Link in new tab "
                                            value="true"/>
                                        <boxFeild
                                            jcr:primaryType="nt:unstructured"
                                            jcr:title="Box List Feild"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                            composite="{Boolean}true"
                                            eaem-show-on-collapse="EAEM.showProductName"
                                            fieldLabel="Box List Feild">
                                            <field
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset"
                                                eaem-nested="NODE_STORE"
                                                name="./boxListFeild">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <column
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <box-list-text
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldLabel="Box List"
                                                                name="./boxList"/>
                                                        </items>
                                                    </column>
                                                </items>
                                            </field>
                                        </boxFeild>
                                    </items>
                                </column>
                            </items>
                        </field>
                    </wealthService>
                </items>
            </column>
        </items>
    </content>
</jcr:root>
