<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Footer Authoring"
    sling:resourceType="cq/gui/components/authoring/dialog"
    helpPath="">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
            <column
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                    <footerLogo
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/accordion">
                        <items jcr:primaryType="nt:unstructured">
                            <container
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Footer logo and copy right"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <footerLogoList
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/accordion">
                                        <items jcr:primaryType="nt:unstructured">
                                            <footerList
                                                jcr:primaryType="nt:unstructured"
                                                jcr:title="&#xa;Footer logo"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                composite="{Boolean}true"
                                                fieldLabel="&#xa;Footer logo">
                                                <field
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/fieldset"
                                                    name="./footorMainlogo">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <column
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <imagePath
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                                    fieldLabel="Image Path"
                                                                    name="./image"
                                                                    rootPath="/content"/>
                                                                <link
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                                    fieldLabel="Enter redirection link"
                                                                    name="./redirectionLink"
                                                                    rootPath="/content"/>
                                                                <openinnewtab
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                                    composite="{Boolean}true"
                                                                    name="./openInNewTab"
                                                                    text="Check for open link in new tab"
                                                                    value="true"/>
                                                                <alternate-name
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                    fieldLabel="Enter Alternate Name of Image"
                                                                    name="./altName"
                                                                    useFixedInlineToolbar="{Boolean}true"/>
                                                                <subLogoList
                                                                    jcr:primaryType="nt:unstructured"
                                                                    jcr:title="Footer List"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                                    composite="{Boolean}true"
                                                                    fieldLabel="App sec">
                                                                    <field
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/fieldset"
                                                                        name="./subLogoList">
                                                                        <items jcr:primaryType="nt:unstructured">
                                                                            <column
                                                                                jcr:primaryType="nt:unstructured"
                                                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                                                <items jcr:primaryType="nt:unstructured">
                                                                                    <imagePath
                                                                                        jcr:primaryType="nt:unstructured"
                                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                                                        fieldLabel="Image Path"
                                                                                        name="./image"
                                                                                        rootPath="/content"/>
                                                                                    <link
                                                                                        jcr:primaryType="nt:unstructured"
                                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                                                        fieldLabel="Enter redirection link"
                                                                                        name="./redirectionLink"
                                                                                        rootPath="/content"/>
                                                                                    <openinnewtab
                                                                                        jcr:primaryType="nt:unstructured"
                                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                                                        composite="{Boolean}true"
                                                                                        name="./openInNewTab"
                                                                                        text="Check for open link in new tab"
                                                                                        value="true"/>
                                                                                    <alternate-name
                                                                                        jcr:primaryType="nt:unstructured"
                                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                                        fieldLabel="Enter Alternate Name of Image"
                                                                                        name="./altName"
                                                                                        useFixedInlineToolbar="{Boolean}true"/>
                                                                                </items>
                                                                            </column>
                                                                        </items>
                                                                    </field>
                                                                </subLogoList>
                                                            </items>
                                                        </column>
                                                    </items>
                                                </field>
                                            </footerList>
                                        </items>
                                    </footerLogoList>
                                    <topHeading
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Enter heading"
                                        name="./followheading"
                                        useFixedInlineToolbar="{Boolean}true"/>
                                    <followUs
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/accordion">
                                        <items jcr:primaryType="nt:unstructured">
                                            <footerList
                                                jcr:primaryType="nt:unstructured"
                                                jcr:title="Follow us link"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                composite="{Boolean}true"
                                                fieldLabel="Follow us link">
                                                <field
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/fieldset"
                                                    name="./socialMedia">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <column
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <imagePath
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                                    fieldLabel="Image Path"
                                                                    name="./imagePath"
                                                                    rootPath="/content"/>
                                                                <link
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                                    fieldLabel="Enter redirection link"
                                                                    name="./redirectionLink"
                                                                    rootPath="/content"/>
                                                                <openinnewtab
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                                    composite="{Boolean}true"
                                                                    name="./openInNewTab"
                                                                    text="Check for open link in new tab"
                                                                    value="true"/>
                                                                <alternate-name
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                    fieldLabel="Enter Alternate Name of Image"
                                                                    name="./altName"
                                                                    useFixedInlineToolbar="{Boolean}true"/>
                                                            </items>
                                                        </column>
                                                    </items>
                                                </field>
                                            </footerList>
                                        </items>
                                    </followUs>
                                </items>
                            </container>
                        </items>
                    </footerLogo>
                </items>
            </column>
        </items>
    </content>
</jcr:root>
