<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Configuration Knowledge Centre Report"
    sling:resourceType="cq/gui/components/authoring/dialog"
    helpPath="{Path}/mnt/overlay/wcm/core/content/sites/components/details.html/apps/tata-capital-website/components/content/wm/wm-footer/README.md">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
            <column
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs">
                <items jcr:primaryType="nt:unstructured">
                    <tabButton
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Tab Button And Content"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        fieldLabel="Tab Button">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <tabButton
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Tab Button"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                        composite="{Boolean}true"
                                        fieldLabel="Tab Button">
                                        <field
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/fieldset"
                                            eaem-nested="NODE_STORE"
                                            name="./tabButtonMulti">
                                            <items jcr:primaryType="nt:unstructured">
                                                <column
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/container">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <tabButton
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="/libs/granite/ui/components/foundation/form/textfield"
                                                            fieldLabel="Enter Tab Button Text"
                                                            name="./tabButtonName"/>
                                                        <active
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                            composite="{Boolean}true"
                                                            name="./checkActive"
                                                            text="Check to Active Tab"
                                                            value="true"/>
                                                        <tab-content
                                                            jcr:primaryType="nt:unstructured"
                                                            jcr:title="Tab Content"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                            composite="{Boolean}true"
                                                            fieldLabel="Tab Content">
                                                            <field
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset"
                                                                eaem-nested="NODE_STORE"
                                                                name="./tabContentMulti">
                                                                <items jcr:primaryType="nt:unstructured">
                                                                    <column
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                                                        <items jcr:primaryType="nt:unstructured">
                                                                            <pdf-year
                                                                                jcr:primaryType="nt:unstructured"
                                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                                fieldLabel="Enter Pdf Year"
                                                                                name="./pdfYear"
                                                                                useFixedInlineToolbar="{Boolean}true"/>
                                                                            <pdf-month
                                                                                jcr:primaryType="nt:unstructured"
                                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                                fieldLabel="Enter Pdf Month"
                                                                                name="./pdfMonth"
                                                                                useFixedInlineToolbar="{Boolean}true"/>
                                                                            <title
                                                                                jcr:primaryType="nt:unstructured"
                                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                                fieldLabel="Title"
                                                                                name="./pdfTitle"
                                                                                useFixedInlineToolbar="{Boolean}true"/>
                                                                            <description
                                                                                jcr:primaryType="nt:unstructured"
                                                                                sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                                                                fieldLabel="Description"
                                                                                name="./description"
                                                                                useFixedInlineToolbar="{Boolean}true">
                                                                                <rtePlugins jcr:primaryType="nt:unstructured">
                                                                                    <format
                                                                                        jcr:primaryType="nt:unstructured"
                                                                                        features="*"/>
                                                                                    <justify
                                                                                        jcr:primaryType="nt:unstructured"
                                                                                        features="*"/>
                                                                                    <lists
                                                                                        jcr:primaryType="nt:unstructured"
                                                                                        features="*"/>
                                                                                    <paraformat
                                                                                        jcr:primaryType="nt:unstructured"
                                                                                        features="*"/>
                                                                                    <links
                                                                                        jcr:primaryType="nt:unstructured"
                                                                                        features="*"/>
                                                                                    <image
                                                                                        jcr:primaryType="nt:unstructured"
                                                                                        features="*"/>
                                                                                    <findreplace
                                                                                        jcr:primaryType="nt:unstructured"
                                                                                        features="*"/>
                                                                                    <subsuperscript
                                                                                        jcr:primaryType="nt:unstructured"
                                                                                        features="*"/>
                                                                                    <spellcheck
                                                                                        jcr:primaryType="nt:unstructured"
                                                                                        features="*"/>
                                                                                    <misctools
                                                                                        jcr:primaryType="nt:unstructured"
                                                                                        features="*">
                                                                                        <specialCharsConfig jcr:primaryType="nt:unstructured">
                                                                                            <chars jcr:primaryType="nt:unstructured">
                                                                                                <copyright
                                                                                                    jcr:primaryType="nt:unstructured"
                                                                                                    entity="&amp;#169;"/>
                                                                                                <trademark
                                                                                                    jcr:primaryType="nt:unstructured"
                                                                                                    entity="&amp;#8482;"/>
                                                                                                <registered
                                                                                                    jcr:primaryType="nt:unstructured"
                                                                                                    entity="&amp;#174;"/>
                                                                                                <emDash
                                                                                                    jcr:primaryType="nt:unstructured"
                                                                                                    entity="&amp;#8212;"/>
                                                                                                <pound
                                                                                                    jcr:primaryType="nt:unstructured"
                                                                                                    entity="&amp;#163;"/>
                                                                                                <nbsp
                                                                                                    jcr:primaryType="nt:unstructured"
                                                                                                    entity="&amp;#160;"/>
                                                                                            </chars>
                                                                                        </specialCharsConfig>
                                                                                    </misctools>
                                                                                    <styles
                                                                                        jcr:primaryType="nt:unstructured"
                                                                                        features="*">
                                                                                        <styles jcr:primaryType="nt:unstructured">
                                                                                            <plainSpan
                                                                                                jcr:primaryType="nt:unstructured"
                                                                                                cssName="defaultSpan"
                                                                                                text="Default (add span tag)"/>
                                                                                            <lead
                                                                                                jcr:primaryType="nt:unstructured"
                                                                                                cssName="lead"
                                                                                                text="Lead"/>
                                                                                        </styles>
                                                                                    </styles>
                                                                                    <table
                                                                                        jcr:primaryType="nt:unstructured"
                                                                                        features="*"/>
                                                                                </rtePlugins>
                                                                                <uiSettings jcr:primaryType="nt:unstructured">
                                                                                    <cui jcr:primaryType="nt:unstructured">
                                                                                        <inline
                                                                                            jcr:primaryType="nt:unstructured"
                                                                                            toolbar="[format#bold,format#italic,format#underline,#justify,#lists,subsuperscript#subscript,subsuperscript#superscript,links#modifylink,links#unlink,links#anchor,#image,findreplace#find,findreplace#replace,spellcheck#checktext,misctools#specialchars,misctools#sourceedit,#styles,#paraformat,table#table]">
                                                                                            <popovers jcr:primaryType="nt:unstructured">
                                                                                                <justify
                                                                                                    jcr:primaryType="nt:unstructured"
                                                                                                    items="[justify#justifyleft,justify#justifycenter,justify#justifyright]"
                                                                                                    ref="justify"/>
                                                                                                <lists
                                                                                                    jcr:primaryType="nt:unstructured"
                                                                                                    items="[lists#unordered,lists#ordered,lists#outdent,lists#indent]"
                                                                                                    ref="lists"/>
                                                                                                <paraformat
                                                                                                    jcr:primaryType="nt:unstructured"
                                                                                                    items="paraformat:getFormats:paraformat-pulldown"
                                                                                                    ref="paraformat"/>
                                                                                                <styles
                                                                                                    jcr:primaryType="nt:unstructured"
                                                                                                    items="styles:getStyles:styles-pulldown"
                                                                                                    ref="styles"/>
                                                                                            </popovers>
                                                                                        </inline>
                                                                                    </cui>
                                                                                </uiSettings>
                                                                            </description>
                                                                            <img
                                                                                jcr:primaryType="nt:unstructured"
                                                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                                                fieldLabel="Enter Image Path"
                                                                                name="./img"
                                                                                rootPath="/content/dam/tata-capital-web"/>
                                                                            <alt-text
                                                                                jcr:primaryType="nt:unstructured"
                                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                                fieldLabel="Image Alt Text"
                                                                                name="./altText"
                                                                                useFixedInlineToolbar="{Boolean}true"/>
                                                                            <read-more-text
                                                                                jcr:primaryType="nt:unstructured"
                                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                                fieldLabel="Enter Read More Button Name"
                                                                                name="./readmoreButtonText"
                                                                                useFixedInlineToolbar="{Boolean}true"/>
                                                                            <read-more-link
                                                                                jcr:primaryType="nt:unstructured"
                                                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                                                fieldLabel="Enter Read More Button Link"
                                                                                name="./readmoreLink"
                                                                                rootPath="/content"/>
                                                                            <newTab
                                                                                jcr:primaryType="nt:unstructured"
                                                                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                                                composite="{Boolean}true"
                                                                                name="./checkNewTab"
                                                                                text="Open in New Tab"
                                                                                value="true"/>
                                                                            <webinar-text
                                                                                jcr:primaryType="nt:unstructured"
                                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                                fieldLabel="Enter Webinar Button Name"
                                                                                name="./webinarButtonText"
                                                                                useFixedInlineToolbar="{Boolean}true"/>
                                                                            <webinar-link
                                                                                jcr:primaryType="nt:unstructured"
                                                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                                                fieldLabel="Enter Webinar Video Link"
                                                                                name="./webinarBtnLink"
                                                                                rootPath="/content"/>
                                                                            <newTab2
                                                                                jcr:primaryType="nt:unstructured"
                                                                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                                                composite="{Boolean}true"
                                                                                name="./checkNewTab2"
                                                                                text="Open in New Tab"
                                                                                value="true"/>
                                                                            <pdf-text
                                                                                jcr:primaryType="nt:unstructured"
                                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                                fieldLabel="Enter Pdf Button Name"
                                                                                name="./pdfButtonText"
                                                                                useFixedInlineToolbar="{Boolean}true"/>
                                                                            <pdf-link
                                                                                jcr:primaryType="nt:unstructured"
                                                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                                                fieldLabel="Enter Pdf Button Link"
                                                                                name="./pdfBtnLink"
                                                                                rootPath="/content"/>
                                                                            <newTab3
                                                                                jcr:primaryType="nt:unstructured"
                                                                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                                                composite="{Boolean}true"
                                                                                name="./checkNewTab3"
                                                                                text="Open in New Tab"
                                                                                value="true"/>
                                                                            <margin
                                                                                jcr:primaryType="nt:unstructured"
                                                                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                                                composite="{Boolean}true"
                                                                                name="./marginBottom"
                                                                                text="Check Card Margin Bottom 30px"
                                                                                value="true"/>
                                                                        </items>
                                                                    </column>
                                                                </items>
                                                            </field>
                                                        </tab-content>
                                                    </items>
                                                </column>
                                            </items>
                                        </field>
                                    </tabButton>
                                </items>
                            </column>
                        </items>
                    </tabButton>
                    <rightFilter
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Right Filter"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <container
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Other website"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <default-year
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Default Year Text"
                                        name="./defaultYear"
                                        useFixedInlineToolbar="{Boolean}true"/>
                                    <filter-items-year
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Years List"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                        composite="{Boolean}true"
                                        fieldLabel="Years List">
                                        <field
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/fieldset"
                                            eaem-nested="NODE_STORE"
                                            name="./rightFilterYear">
                                            <items jcr:primaryType="nt:unstructured">
                                                <column
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/container">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <filter-text
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldLabel="Filter Items Text"
                                                            name="./filterItemsText"
                                                            useFixedInlineToolbar="{Boolean}true"/>
                                                        <filter-data
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldLabel="Filter Items Data"
                                                            name="./filterItemsData"
                                                            useFixedInlineToolbar="{Boolean}true"/>
                                                    </items>
                                                </column>
                                            </items>
                                        </field>
                                    </filter-items-year>
                                    <default-month
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Default Month Text"
                                        name="./defaultMonth"
                                        useFixedInlineToolbar="{Boolean}true"/>
                                    <filter-items-months
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Months List"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                        composite="{Boolean}true"
                                        fieldLabel="Months List">
                                        <field
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/fieldset"
                                            eaem-nested="NODE_STORE"
                                            name="./filterMonths">
                                            <items jcr:primaryType="nt:unstructured">
                                                <column
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/container">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <filter-text
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldLabel="Filter Items Text"
                                                            name="./filterItemsText"
                                                            useFixedInlineToolbar="{Boolean}true"/>
                                                        <filter-data
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldLabel="Filter Items Data"
                                                            name="./filterItemsData"
                                                            useFixedInlineToolbar="{Boolean}true"/>
                                                    </items>
                                                </column>
                                            </items>
                                        </field>
                                    </filter-items-months>
                                </items>
                            </container>
                        </items>
                    </rightFilter>
                </items>
            </column>
        </items>
    </content>
</jcr:root>
