                                                </assetpicker>
                                                <preview
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="cq/gui/components/siteadmin/admin/pagepreview"/>
                                            </thumbnail>
                                        </items>
                                    </thumbnail>
                                </items>
                            </column>
                        </items>
